import java.util.*;
class factorial
{
public static void main(String args[])
{
System.out.println("enter n value: ");
Scanner sc=new Scanner(System.in);
int n=sc.nextInt();
int z=0;
int r=(int)(Math.log(n)/Math.log(5));
for(int i=1;i<=r;i++)
{
z+=n/Math.pow(5,i);
}
System.out.println("Factorial= "+z);
}
}
